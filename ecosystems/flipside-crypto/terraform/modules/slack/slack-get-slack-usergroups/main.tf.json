{
  "data": {
    "env_sensitive": {
      "SLACK_BOT_TOKEN": {
        "id": "SLACK_BOT_TOKEN",
        "required": true
      },
      "SLACK_TOKEN": {
        "id": "SLACK_TOKEN",
        "required": true
      }
    },
    "external": {
      "default": {
        "program": [
          "tm_cli",
          "get_slack_usergroups"
        ],
        "query": {
          "SLACK_BOT_TOKEN": "${data.env_sensitive.SLACK_BOT_TOKEN.value}",
          "SLACK_TOKEN": "${data.env_sensitive.SLACK_TOKEN.value}",
          "channel_identifiers_file": "${try(nonsensitive(var.channel_identifiers_file), var.channel_identifiers_file)}",
          "checksum": "${try(nonsensitive(var.checksum), var.checksum)}",
          "debug_markers": "${try(nonsensitive(jsonencode(var.debug_markers)), jsonencode(var.debug_markers))}",
          "expand_users": "${try(nonsensitive(var.expand_users), var.expand_users)}",
          "include_count": "${try(nonsensitive(var.include_count), var.include_count)}",
          "include_disabled": "${try(nonsensitive(var.include_disabled), var.include_disabled)}",
          "include_users": "${try(nonsensitive(var.include_users), var.include_users)}",
          "log_file_name": "${try(nonsensitive(var.log_file_name), var.log_file_name)}",
          "params": "${try(nonsensitive(base64encode(jsonencode(var.params))), base64encode(jsonencode(var.params)))}",
          "prefix": "${try(nonsensitive(var.prefix), var.prefix)}",
          "sort_by_name": "${try(nonsensitive(var.sort_by_name), var.sort_by_name)}",
          "team_id": "${try(nonsensitive(var.team_id), var.team_id)}",
          "user_identifiers_file": "${try(nonsensitive(var.user_identifiers_file), var.user_identifiers_file)}",
          "verbose": "${try(nonsensitive(var.verbose), var.verbose)}",
          "verbosity": "${try(nonsensitive(var.verbosity), var.verbosity)}"
        }
      }
    }
  },
  "locals": {
    "results": "${jsondecode(base64decode(data.external.default.result[\"user_groups\"]))}"
  },
  "output": {
    "user_groups": {
      "description": "Data query results",
      "value": "${local.results}"
    }
  },
  "terraform": {
    "required_providers": {
      "env": {
        "source": "tcarreira/env",
        "version": ">=0.2.0"
      },
      "external": {
        "source": "hashicorp/external",
        "version": ">=2.3.1"
      }
    },
    "required_version": ">=1.6"
  },
  "variable": {
    "channel_identifiers_file": {
      "default": null,
      "type": "string"
    },
    "checksum": {
      "default": "",
      "description": "Optional checksum to use for triggering resource updates",
      "type": "string"
    },
    "debug_markers": {
      "default": [],
      "description": "Identifiers to generate verbose debug statements for",
      "type": "list(string)"
    },
    "expand_users": {
      "default": false,
      "type": "bool"
    },
    "include_count": {
      "default": null,
      "type": "any"
    },
    "include_disabled": {
      "default": null,
      "type": "any"
    },
    "include_users": {
      "default": null,
      "type": "any"
    },
    "log_file_name": {
      "default": "run.log",
      "description": "Port library parameter",
      "type": "string"
    },
    "params": {
      "default": {},
      "description": "Override for multiple params",
      "type": "any"
    },
    "prefix": {
      "default": null,
      "type": "bool"
    },
    "sort_by_name": {
      "default": false,
      "type": "bool"
    },
    "team_id": {
      "default": null,
      "type": "string"
    },
    "user_identifiers_file": {
      "default": null,
      "type": "string"
    },
    "verbose": {
      "default": false,
      "description": "Port library parameter",
      "type": "bool"
    },
    "verbosity": {
      "default": 1,
      "description": "Port library parameter",
      "type": "number"
    }
  }
}