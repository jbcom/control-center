{
  "data": {
    "env_sensitive": {
      "GOOGLE_SERVICE_ACCOUNT": {
        "id": "GOOGLE_SERVICE_ACCOUNT",
        "required": true
      },
      "SLACK_BOT_TOKEN": {
        "id": "SLACK_BOT_TOKEN",
        "required": true
      },
      "SLACK_TOKEN": {
        "id": "SLACK_TOKEN",
        "required": true
      },
      "ZOOM_ACCOUNT_ID": {
        "id": "ZOOM_ACCOUNT_ID",
        "required": true
      },
      "ZOOM_CLIENT_ID": {
        "id": "ZOOM_CLIENT_ID",
        "required": true
      },
      "ZOOM_CLIENT_SECRET": {
        "id": "ZOOM_CLIENT_SECRET",
        "required": true
      }
    }
  },
  "resource": {
    "terraform_data": {
      "default": {
        "provisioner": [
          {
            "local-exec": {
              "command": "tm_cli sync_flipsidecrypto_users_and_groups",
              "environment": {
                "GOOGLE_SERVICE_ACCOUNT": "${data.env_sensitive.GOOGLE_SERVICE_ACCOUNT.value}",
                "SLACK_BOT_TOKEN": "${data.env_sensitive.SLACK_BOT_TOKEN.value}",
                "SLACK_TOKEN": "${data.env_sensitive.SLACK_TOKEN.value}",
                "ZOOM_ACCOUNT_ID": "${data.env_sensitive.ZOOM_ACCOUNT_ID.value}",
                "ZOOM_CLIENT_ID": "${data.env_sensitive.ZOOM_CLIENT_ID.value}",
                "ZOOM_CLIENT_SECRET": "${data.env_sensitive.ZOOM_CLIENT_SECRET.value}",
                "checksum": "${self.triggers_replace.checksum}",
                "debug_markers": "${self.triggers_replace.debug_markers}",
                "group_members_assigned_aws_accounts": "${self.triggers_replace.group_members_assigned_aws_accounts}",
                "log_file_name": "${self.triggers_replace.log_file_name}",
                "onboard_users": "${self.triggers_replace.onboard_users}",
                "params": "${self.triggers_replace.params}",
                "verbose": "${self.triggers_replace.verbose}",
                "verbosity": "${self.triggers_replace.verbosity}"
              }
            }
          }
        ],
        "triggers_replace": {
          "checksum": "${try(nonsensitive(var.checksum), var.checksum)}",
          "debug_markers": "${try(nonsensitive(jsonencode(var.debug_markers)), jsonencode(var.debug_markers))}",
          "group_members_assigned_aws_accounts": "${try(nonsensitive(jsonencode(var.group_members_assigned_aws_accounts)), jsonencode(var.group_members_assigned_aws_accounts))}",
          "log_file_name": "${try(nonsensitive(var.log_file_name), var.log_file_name)}",
          "onboard_users": "${try(nonsensitive(base64encode(jsonencode(var.onboard_users))), base64encode(jsonencode(var.onboard_users)))}",
          "params": "${try(nonsensitive(base64encode(jsonencode(var.params))), base64encode(jsonencode(var.params)))}",
          "verbose": "${try(nonsensitive(var.verbose), var.verbose)}",
          "verbosity": "${try(nonsensitive(var.verbosity), var.verbosity)}"
        }
      }
    }
  },
  "terraform": {
    "required_providers": {
      "env": {
        "source": "tcarreira/env",
        "version": ">=0.2.0"
      }
    },
    "required_version": ">=1.6"
  },
  "variable": {
    "checksum": {
      "default": "",
      "description": "Optional checksum to use for triggering resource updates",
      "type": "string"
    },
    "debug_markers": {
      "default": [],
      "description": "Identifiers to generate verbose debug statements for",
      "type": "list(string)"
    },
    "group_members_assigned_aws_accounts": {
      "default": [],
      "type": "any"
    },
    "log_file_name": {
      "default": "run.log",
      "description": "Port library parameter",
      "type": "string"
    },
    "onboard_users": {
      "default": {},
      "type": "any"
    },
    "params": {
      "default": {},
      "description": "Override for multiple params",
      "type": "any"
    },
    "verbose": {
      "default": false,
      "description": "Port library parameter",
      "type": "bool"
    },
    "verbosity": {
      "default": 1,
      "description": "Port library parameter",
      "type": "number"
    }
  }
}