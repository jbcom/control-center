{
  "data": {
    "env_sensitive": {
      "GOOGLE_SERVICE_ACCOUNT": {
        "id": "GOOGLE_SERVICE_ACCOUNT",
        "required": true
      }
    },
    "external": {
      "default": {
        "program": [
          "tm_cli",
          "get_dead_google_projects"
        ],
        "query": {
          "GOOGLE_SERVICE_ACCOUNT": "${data.env_sensitive.GOOGLE_SERVICE_ACCOUNT.value}",
          "billing_dataset": "${try(nonsensitive(var.billing_dataset), var.billing_dataset)}",
          "checksum": "${try(nonsensitive(var.checksum), var.checksum)}",
          "dataset_name": "${try(nonsensitive(var.dataset_name), var.dataset_name)}",
          "debug_markers": "${try(nonsensitive(jsonencode(var.debug_markers)), jsonencode(var.debug_markers))}",
          "inactivity_period_days": "${try(nonsensitive(var.inactivity_period_days), var.inactivity_period_days)}",
          "log_file_name": "${try(nonsensitive(var.log_file_name), var.log_file_name)}",
          "params": "${try(nonsensitive(base64encode(jsonencode(var.params))), base64encode(jsonencode(var.params)))}",
          "project_id": "${try(nonsensitive(var.project_id), var.project_id)}",
          "service_account_file": "${try(nonsensitive(base64encode(jsonencode(var.service_account_file))), base64encode(jsonencode(var.service_account_file)))}",
          "verbose": "${try(nonsensitive(var.verbose), var.verbose)}",
          "verbosity": "${try(nonsensitive(var.verbosity), var.verbosity)}"
        }
      }
    }
  },
  "locals": {
    "results": "${jsondecode(base64decode(data.external.default.result[\"dead_projects\"]))}"
  },
  "output": {
    "dead_projects": {
      "description": "Data query results",
      "value": "${local.results}"
    }
  },
  "terraform": {
    "required_providers": {
      "env": {
        "source": "tcarreira/env",
        "version": ">=0.2.0"
      },
      "external": {
        "source": "hashicorp/external",
        "version": ">=2.3.1"
      }
    },
    "required_version": ">=1.6"
  },
  "variable": {
    "billing_dataset": {
      "default": null,
      "type": "string"
    },
    "checksum": {
      "default": "",
      "description": "Optional checksum to use for triggering resource updates",
      "type": "string"
    },
    "dataset_name": {
      "default": "billing_dataset",
      "type": "string"
    },
    "debug_markers": {
      "default": [],
      "description": "Identifiers to generate verbose debug statements for",
      "type": "list(string)"
    },
    "inactivity_period_days": {
      "default": 30,
      "type": "integer"
    },
    "log_file_name": {
      "default": "run.log",
      "description": "Port library parameter",
      "type": "string"
    },
    "params": {
      "default": {},
      "description": "Override for multiple params",
      "type": "any"
    },
    "project_id": {
      "default": "flipside-security-admin",
      "type": "string"
    },
    "service_account_file": {
      "default": null,
      "type": "any"
    },
    "verbose": {
      "default": false,
      "description": "Port library parameter",
      "type": "bool"
    },
    "verbosity": {
      "default": 1,
      "description": "Port library parameter",
      "type": "number"
    }
  }
}