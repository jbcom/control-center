{
  "data": {
    "env_sensitive": {
      "GOOGLE_SERVICE_ACCOUNT": {
        "id": "GOOGLE_SERVICE_ACCOUNT",
        "required": true
      }
    },
    "external": {
      "default": {
        "program": [
          "tm_cli",
          "get_google_groups"
        ],
        "query": {
          "GOOGLE_SERVICE_ACCOUNT": "${data.env_sensitive.GOOGLE_SERVICE_ACCOUNT.value}",
          "checksum": "${try(nonsensitive(var.checksum), var.checksum)}",
          "debug_markers": "${try(nonsensitive(jsonencode(var.debug_markers)), jsonencode(var.debug_markers))}",
          "flatten_members": "${try(nonsensitive(var.flatten_members), var.flatten_members)}",
          "group_keys": "${try(nonsensitive(jsonencode(var.group_keys)), jsonencode(var.group_keys))}",
          "group_names": "${try(nonsensitive(jsonencode(var.group_names)), jsonencode(var.group_names))}",
          "log_file_name": "${try(nonsensitive(var.log_file_name), var.log_file_name)}",
          "members_only": "${try(nonsensitive(var.members_only), var.members_only)}",
          "only_status_for_members": "${try(nonsensitive(var.only_status_for_members), var.only_status_for_members)}",
          "only_type_for_members": "${try(nonsensitive(var.only_type_for_members), var.only_type_for_members)}",
          "params": "${try(nonsensitive(base64encode(jsonencode(var.params))), base64encode(jsonencode(var.params)))}",
          "sort_by_name": "${try(nonsensitive(var.sort_by_name), var.sort_by_name)}",
          "unhump_groups": "${try(nonsensitive(var.unhump_groups), var.unhump_groups)}",
          "user_key": "${try(nonsensitive(var.user_key), var.user_key)}",
          "verbose": "${try(nonsensitive(var.verbose), var.verbose)}",
          "verbosity": "${try(nonsensitive(var.verbosity), var.verbosity)}"
        }
      }
    }
  },
  "locals": {
    "results": "${jsondecode(base64decode(data.external.default.result[\"groups\"]))}"
  },
  "output": {
    "groups": {
      "description": "Data query results",
      "value": "${local.results}"
    }
  },
  "terraform": {
    "required_providers": {
      "env": {
        "source": "tcarreira/env",
        "version": ">=0.2.0"
      },
      "external": {
        "source": "hashicorp/external",
        "version": ">=2.3.1"
      }
    },
    "required_version": ">=1.6"
  },
  "variable": {
    "checksum": {
      "default": "",
      "description": "Optional checksum to use for triggering resource updates",
      "type": "string"
    },
    "debug_markers": {
      "default": [],
      "description": "Identifiers to generate verbose debug statements for",
      "type": "list(string)"
    },
    "flatten_members": {
      "default": false,
      "type": "bool"
    },
    "group_keys": {
      "default": [],
      "type": "any"
    },
    "group_names": {
      "default": null,
      "type": "any"
    },
    "log_file_name": {
      "default": "run.log",
      "description": "Port library parameter",
      "type": "string"
    },
    "members_only": {
      "default": false,
      "type": "bool"
    },
    "only_status_for_members": {
      "default": null,
      "type": "string"
    },
    "only_type_for_members": {
      "default": null,
      "type": "string"
    },
    "params": {
      "default": {},
      "description": "Override for multiple params",
      "type": "any"
    },
    "sort_by_name": {
      "default": false,
      "type": "bool"
    },
    "unhump_groups": {
      "default": true,
      "type": "bool"
    },
    "user_key": {
      "default": null,
      "type": "string"
    },
    "verbose": {
      "default": false,
      "description": "Port library parameter",
      "type": "bool"
    },
    "verbosity": {
      "default": 1,
      "description": "Port library parameter",
      "type": "number"
    }
  }
}