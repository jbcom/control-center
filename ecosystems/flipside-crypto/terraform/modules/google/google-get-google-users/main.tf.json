{
  "data": {
    "env_sensitive": {
      "GOOGLE_SERVICE_ACCOUNT": {
        "id": "GOOGLE_SERVICE_ACCOUNT",
        "required": true
      }
    },
    "external": {
      "default": {
        "program": [
          "tm_cli",
          "get_google_users"
        ],
        "query": {
          "GOOGLE_SERVICE_ACCOUNT": "${data.env_sensitive.GOOGLE_SERVICE_ACCOUNT.value}",
          "active_only": "${try(nonsensitive(var.active_only), var.active_only)}",
          "allowed_ous": "${try(nonsensitive(jsonencode(var.allowed_ous)), jsonencode(var.allowed_ous))}",
          "checksum": "${try(nonsensitive(var.checksum), var.checksum)}",
          "debug_markers": "${try(nonsensitive(jsonencode(var.debug_markers)), jsonencode(var.debug_markers))}",
          "denied_ous": "${try(nonsensitive(jsonencode(var.denied_ous)), jsonencode(var.denied_ous))}",
          "flatten_name": "${try(nonsensitive(var.flatten_name), var.flatten_name)}",
          "include_bots": "${try(nonsensitive(var.include_bots), var.include_bots)}",
          "log_file_name": "${try(nonsensitive(var.log_file_name), var.log_file_name)}",
          "params": "${try(nonsensitive(base64encode(jsonencode(var.params))), base64encode(jsonencode(var.params)))}",
          "unhump_users": "${try(nonsensitive(var.unhump_users), var.unhump_users)}",
          "verbose": "${try(nonsensitive(var.verbose), var.verbose)}",
          "verbosity": "${try(nonsensitive(var.verbosity), var.verbosity)}"
        }
      }
    }
  },
  "locals": {
    "results": "${jsondecode(base64decode(data.external.default.result[\"users\"]))}"
  },
  "output": {
    "users": {
      "description": "Data query results",
      "value": "${local.results}"
    }
  },
  "terraform": {
    "required_providers": {
      "env": {
        "source": "tcarreira/env",
        "version": ">=0.2.0"
      },
      "external": {
        "source": "hashicorp/external",
        "version": ">=2.3.1"
      }
    },
    "required_version": ">=1.6"
  },
  "variable": {
    "active_only": {
      "default": false,
      "type": "bool"
    },
    "allowed_ous": {
      "default": [],
      "type": "any"
    },
    "checksum": {
      "default": "",
      "description": "Optional checksum to use for triggering resource updates",
      "type": "string"
    },
    "debug_markers": {
      "default": [],
      "description": "Identifiers to generate verbose debug statements for",
      "type": "list(string)"
    },
    "denied_ous": {
      "default": [],
      "type": "any"
    },
    "flatten_name": {
      "default": false,
      "type": "bool"
    },
    "include_bots": {
      "default": true,
      "type": "bool"
    },
    "log_file_name": {
      "default": "run.log",
      "description": "Port library parameter",
      "type": "string"
    },
    "params": {
      "default": {},
      "description": "Override for multiple params",
      "type": "any"
    },
    "unhump_users": {
      "default": true,
      "type": "bool"
    },
    "verbose": {
      "default": false,
      "description": "Port library parameter",
      "type": "bool"
    },
    "verbosity": {
      "default": 1,
      "description": "Port library parameter",
      "type": "number"
    }
  }
}