{
  "data": {
    "external": {
      "default": {
        "program": [
          "tm_cli",
          "build_github_actions_workflow"
        ],
        "query": {
          "checksum": "${try(nonsensitive(var.checksum), var.checksum)}",
          "concurrency_group": "${try(nonsensitive(var.concurrency_group), var.concurrency_group)}",
          "debug_markers": "${try(nonsensitive(jsonencode(var.debug_markers)), jsonencode(var.debug_markers))}",
          "environment_variables": "${try(nonsensitive(base64encode(jsonencode(var.environment_variables))), base64encode(jsonencode(var.environment_variables)))}",
          "events": "${try(nonsensitive(base64encode(jsonencode(var.events))), base64encode(jsonencode(var.events)))}",
          "inputs": "${try(nonsensitive(base64encode(jsonencode(var.inputs))), base64encode(jsonencode(var.inputs)))}",
          "jobs": "${try(nonsensitive(base64encode(jsonencode(var.jobs))), base64encode(jsonencode(var.jobs)))}",
          "log_file_name": "${try(nonsensitive(var.log_file_name), var.log_file_name)}",
          "params": "${try(nonsensitive(base64encode(jsonencode(var.params))), base64encode(jsonencode(var.params)))}",
          "pull_requests": "${try(nonsensitive(base64encode(jsonencode(var.pull_requests))), base64encode(jsonencode(var.pull_requests)))}",
          "secrets": "${try(nonsensitive(base64encode(jsonencode(var.secrets))), base64encode(jsonencode(var.secrets)))}",
          "triggers": "${try(nonsensitive(base64encode(jsonencode(var.triggers))), base64encode(jsonencode(var.triggers)))}",
          "use_oidc_auth": "${try(nonsensitive(var.use_oidc_auth), var.use_oidc_auth)}",
          "verbose": "${try(nonsensitive(var.verbose), var.verbose)}",
          "verbosity": "${try(nonsensitive(var.verbosity), var.verbosity)}",
          "workflow_name": "${try(nonsensitive(var.workflow_name), var.workflow_name)}"
        }
      }
    }
  },
  "locals": {
    "results": "${data.external.default.result[\"workflow\"]}"
  },
  "output": {
    "workflow": {
      "description": "Data query results",
      "value": "${local.results}"
    }
  },
  "terraform": {
    "required_providers": {
      "env": {
        "source": "tcarreira/env",
        "version": ">=0.2.0"
      },
      "external": {
        "source": "hashicorp/external",
        "version": ">=2.3.1"
      }
    },
    "required_version": ">=1.6"
  },
  "variable": {
    "checksum": {
      "default": "",
      "description": "Optional checksum to use for triggering resource updates",
      "type": "string"
    },
    "concurrency_group": {
      "default": null,
      "type": "string"
    },
    "debug_markers": {
      "default": [],
      "description": "Identifiers to generate verbose debug statements for",
      "type": "list(string)"
    },
    "environment_variables": {
      "default": {},
      "type": "any"
    },
    "events": {
      "default": {},
      "type": "any"
    },
    "inputs": {
      "default": {},
      "type": "any"
    },
    "jobs": {
      "type": "any"
    },
    "log_file_name": {
      "default": "run.log",
      "description": "Port library parameter",
      "type": "string"
    },
    "params": {
      "default": {},
      "description": "Override for multiple params",
      "type": "any"
    },
    "pull_requests": {
      "default": {},
      "type": "any"
    },
    "secrets": {
      "default": {},
      "type": "any"
    },
    "triggers": {
      "default": {},
      "type": "any"
    },
    "use_oidc_auth": {
      "default": false,
      "type": "bool"
    },
    "verbose": {
      "default": false,
      "description": "Port library parameter",
      "type": "bool"
    },
    "verbosity": {
      "default": 1,
      "description": "Port library parameter",
      "type": "number"
    },
    "workflow_name": {
      "type": "string"
    }
  }
}