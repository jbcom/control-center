{
  "data": {
    "env_sensitive": {
      "ACTIONS_ID_TOKEN_REQUEST_TOKEN": {
        "id": "ACTIONS_ID_TOKEN_REQUEST_TOKEN",
        "required": false
      },
      "GITHUB_TOKEN": {
        "id": "GITHUB_TOKEN",
        "required": true
      }
    },
    "env_var": {
      "ACTIONS_ID_TOKEN_REQUEST_URL": {
        "id": "ACTIONS_ID_TOKEN_REQUEST_URL",
        "required": false
      },
      "GITHUB_ACTIONS": {
        "id": "GITHUB_ACTIONS",
        "required": false
      },
      "GITHUB_BRANCH": {
        "id": "GITHUB_BRANCH",
        "required": false
      },
      "GITHUB_OWNER": {
        "id": "GITHUB_OWNER",
        "required": false
      },
      "GITHUB_REPO": {
        "id": "GITHUB_REPO",
        "required": false
      }
    },
    "external": {
      "default": {
        "program": [
          "tm_cli",
          "scan_dir"
        ],
        "query": {
          "ACTIONS_ID_TOKEN_REQUEST_TOKEN": "${data.env_sensitive.ACTIONS_ID_TOKEN_REQUEST_TOKEN.value}",
          "ACTIONS_ID_TOKEN_REQUEST_URL": "${data.env_var.ACTIONS_ID_TOKEN_REQUEST_URL.value}",
          "GITHUB_ACTIONS": "${data.env_var.GITHUB_ACTIONS.value}",
          "GITHUB_BRANCH": "${data.env_var.GITHUB_BRANCH.value}",
          "GITHUB_OWNER": "${data.env_var.GITHUB_OWNER.value}",
          "GITHUB_REPO": "${data.env_var.GITHUB_REPO.value}",
          "GITHUB_TOKEN": "${data.env_sensitive.GITHUB_TOKEN.value}",
          "allowed_extensions": "${try(nonsensitive(jsonencode(var.allowed_extensions)), jsonencode(var.allowed_extensions))}",
          "checksum": "${try(nonsensitive(var.checksum), var.checksum)}",
          "debug_markers": "${try(nonsensitive(jsonencode(var.debug_markers)), jsonencode(var.debug_markers))}",
          "decode": "${try(nonsensitive(var.decode), var.decode)}",
          "denied_extensions": "${try(nonsensitive(jsonencode(var.denied_extensions)), jsonencode(var.denied_extensions))}",
          "files_glob": "${try(nonsensitive(var.files_glob), var.files_glob)}",
          "files_match": "${try(nonsensitive(var.files_match), var.files_match)}",
          "files_path": "${try(nonsensitive(var.files_path), var.files_path)}",
          "flatten": "${try(nonsensitive(var.flatten), var.flatten)}",
          "log_file_name": "${try(nonsensitive(var.log_file_name), var.log_file_name)}",
          "max_sanitize_depth": "${try(nonsensitive(var.max_sanitize_depth), var.max_sanitize_depth)}",
          "params": "${try(nonsensitive(base64encode(jsonencode(var.params))), base64encode(jsonencode(var.params)))}",
          "paths_only": "${try(nonsensitive(var.paths_only), var.paths_only)}",
          "recursive": "${try(nonsensitive(var.recursive), var.recursive)}",
          "reject_dotfiles": "${try(nonsensitive(var.reject_dotfiles), var.reject_dotfiles)}",
          "replace_chars_in_key_using": "${try(nonsensitive(var.replace_chars_in_key_using), var.replace_chars_in_key_using)}",
          "replace_chars_in_key_with": "${try(nonsensitive(var.replace_chars_in_key_with), var.replace_chars_in_key_with)}",
          "repository_name": "${try(nonsensitive(var.repository_name), var.repository_name)}",
          "repository_owner": "${try(nonsensitive(var.repository_owner), var.repository_owner)}",
          "sanitize_keys": "${try(nonsensitive(var.sanitize_keys), var.sanitize_keys)}",
          "stem_only": "${try(nonsensitive(var.stem_only), var.stem_only)}",
          "verbose": "${try(nonsensitive(var.verbose), var.verbose)}",
          "verbosity": "${try(nonsensitive(var.verbosity), var.verbosity)}"
        }
      }
    }
  },
  "locals": {
    "results": "${jsondecode(base64decode(data.external.default.result[\"tree\"]))}"
  },
  "output": {
    "tree": {
      "description": "Data query results",
      "value": "${local.results}"
    }
  },
  "terraform": {
    "required_providers": {
      "env": {
        "source": "tcarreira/env",
        "version": ">=0.2.0"
      },
      "external": {
        "source": "hashicorp/external",
        "version": ">=2.3.1"
      }
    },
    "required_version": ">=1.6"
  },
  "variable": {
    "allowed_extensions": {
      "default": [],
      "type": "any"
    },
    "checksum": {
      "default": "",
      "description": "Optional checksum to use for triggering resource updates",
      "type": "string"
    },
    "debug_markers": {
      "default": [],
      "description": "Identifiers to generate verbose debug statements for",
      "type": "list(string)"
    },
    "decode": {
      "default": true,
      "type": "bool"
    },
    "denied_extensions": {
      "default": [],
      "type": "any"
    },
    "files_glob": {
      "default": "*",
      "type": "string"
    },
    "files_match": {
      "default": null,
      "type": "string"
    },
    "files_path": {
      "type": "string"
    },
    "flatten": {
      "default": false,
      "type": "bool"
    },
    "log_file_name": {
      "default": "run.log",
      "description": "Port library parameter",
      "type": "string"
    },
    "max_sanitize_depth": {
      "default": null,
      "type": "number"
    },
    "params": {
      "default": {},
      "description": "Override for multiple params",
      "type": "any"
    },
    "paths_only": {
      "default": false,
      "type": "bool"
    },
    "recursive": {
      "default": true,
      "type": "bool"
    },
    "reject_dotfiles": {
      "default": true,
      "type": "bool"
    },
    "replace_chars_in_key_using": {
      "default": null,
      "type": "any"
    },
    "replace_chars_in_key_with": {
      "default": null,
      "type": "any"
    },
    "repository_name": {
      "default": null,
      "type": "string"
    },
    "repository_owner": {
      "default": null,
      "type": "string"
    },
    "sanitize_keys": {
      "default": false,
      "type": "bool"
    },
    "stem_only": {
      "default": false,
      "type": "bool"
    },
    "verbose": {
      "default": false,
      "description": "Port library parameter",
      "type": "bool"
    },
    "verbosity": {
      "default": 1,
      "description": "Port library parameter",
      "type": "number"
    }
  }
}