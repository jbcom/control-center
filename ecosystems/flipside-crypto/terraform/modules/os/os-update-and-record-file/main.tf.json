{
  "resource": {
    "terraform_data": {
      "default": {
        "provisioner": [
          {
            "local-exec": {
              "command": "tm_cli update_and_record_file",
              "environment": {
                "allow_empty": "${self.triggers_replace.allow_empty}",
                "allow_encoding": "${self.triggers_replace.allow_encoding}",
                "checksum": "${self.triggers_replace.checksum}",
                "debug_markers": "${self.triggers_replace.debug_markers}",
                "file_data": "${self.triggers_replace.file_data}",
                "file_path": "${self.triggers_replace.file_path}",
                "json_data": "${self.triggers_replace.json_data}",
                "log_file_name": "${self.triggers_replace.log_file_name}",
                "params": "${self.triggers_replace.params}",
                "verbose": "${self.triggers_replace.verbose}",
                "verbosity": "${self.triggers_replace.verbosity}",
                "yaml_data": "${self.triggers_replace.yaml_data}"
              }
            }
          }
        ],
        "triggers_replace": {
          "allow_empty": "${try(nonsensitive(var.allow_empty), var.allow_empty)}",
          "allow_encoding": "${try(nonsensitive(var.allow_encoding), var.allow_encoding)}",
          "checksum": "${try(nonsensitive(var.checksum), var.checksum)}",
          "debug_markers": "${try(nonsensitive(jsonencode(var.debug_markers)), jsonencode(var.debug_markers))}",
          "file_data": "${try(nonsensitive(base64encode(jsonencode(var.file_data))), base64encode(jsonencode(var.file_data)))}",
          "file_path": "${try(nonsensitive(var.file_path), var.file_path)}",
          "json_data": "${try(nonsensitive(var.json_data), var.json_data)}",
          "log_file_name": "${try(nonsensitive(var.log_file_name), var.log_file_name)}",
          "params": "${try(nonsensitive(base64encode(jsonencode(var.params))), base64encode(jsonencode(var.params)))}",
          "verbose": "${try(nonsensitive(var.verbose), var.verbose)}",
          "verbosity": "${try(nonsensitive(var.verbosity), var.verbosity)}",
          "yaml_data": "${try(nonsensitive(var.yaml_data), var.yaml_data)}"
        }
      }
    }
  },
  "terraform": {
    "required_providers": {
      "env": {
        "source": "tcarreira/env",
        "version": ">=0.2.0"
      }
    },
    "required_version": ">=1.6"
  },
  "variable": {
    "allow_empty": {
      "default": false,
      "type": "bool"
    },
    "allow_encoding": {
      "default": null,
      "type": "any"
    },
    "checksum": {
      "default": "",
      "description": "Optional checksum to use for triggering resource updates",
      "type": "string"
    },
    "debug_markers": {
      "default": [],
      "description": "Identifiers to generate verbose debug statements for",
      "type": "list(string)"
    },
    "file_data": {
      "default": null,
      "type": "any"
    },
    "file_path": {
      "type": "string"
    },
    "json_data": {
      "default": null,
      "type": "string"
    },
    "log_file_name": {
      "default": "run.log",
      "description": "Port library parameter",
      "type": "string"
    },
    "params": {
      "default": {},
      "description": "Override for multiple params",
      "type": "any"
    },
    "verbose": {
      "default": false,
      "description": "Port library parameter",
      "type": "bool"
    },
    "verbosity": {
      "default": 1,
      "description": "Port library parameter",
      "type": "number"
    },
    "yaml_data": {
      "default": null,
      "type": "string"
    }
  }
}