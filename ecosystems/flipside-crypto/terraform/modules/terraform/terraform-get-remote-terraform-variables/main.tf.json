{
  "data": {
    "external": {
      "default": {
        "program": [
          "tm_cli",
          "get_remote_terraform_variables"
        ],
        "query": {
          "checksum": "${try(nonsensitive(var.checksum), var.checksum)}",
          "debug_markers": "${try(nonsensitive(jsonencode(var.debug_markers)), jsonencode(var.debug_markers))}",
          "defaults": "${try(nonsensitive(base64encode(jsonencode(var.defaults))), base64encode(jsonencode(var.defaults)))}",
          "local_module_source": "${try(nonsensitive(var.local_module_source), var.local_module_source)}",
          "log_file_name": "${try(nonsensitive(var.log_file_name), var.log_file_name)}",
          "map_name_to": "${try(nonsensitive(base64encode(jsonencode(var.map_name_to))), base64encode(jsonencode(var.map_name_to)))}",
          "map_sanitized_name_to": "${try(nonsensitive(base64encode(jsonencode(var.map_sanitized_name_to))), base64encode(jsonencode(var.map_sanitized_name_to)))}",
          "overrides": "${try(nonsensitive(base64encode(jsonencode(var.overrides))), base64encode(jsonencode(var.overrides)))}",
          "parameter_generators": "${try(nonsensitive(base64encode(jsonencode(var.parameter_generators))), base64encode(jsonencode(var.parameter_generators)))}",
          "params": "${try(nonsensitive(base64encode(jsonencode(var.params))), base64encode(jsonencode(var.params)))}",
          "repository_name": "${try(nonsensitive(var.repository_name), var.repository_name)}",
          "repository_tag": "${try(nonsensitive(var.repository_tag), var.repository_tag)}",
          "requires_github_authentication": "${try(nonsensitive(var.requires_github_authentication), var.requires_github_authentication)}",
          "variable_files": "${try(nonsensitive(jsonencode(var.variable_files)), jsonencode(var.variable_files))}",
          "verbose": "${try(nonsensitive(var.verbose), var.verbose)}",
          "verbosity": "${try(nonsensitive(var.verbosity), var.verbosity)}"
        }
      }
    }
  },
  "locals": {
    "results": "${jsondecode(base64decode(data.external.default.result[\"variables\"]))}"
  },
  "output": {
    "variables": {
      "description": "Data query results",
      "value": "${local.results}"
    }
  },
  "terraform": {
    "required_providers": {
      "env": {
        "source": "tcarreira/env",
        "version": ">=0.2.0"
      },
      "external": {
        "source": "hashicorp/external",
        "version": ">=2.3.1"
      }
    },
    "required_version": ">=1.6"
  },
  "variable": {
    "checksum": {
      "default": "",
      "description": "Optional checksum to use for triggering resource updates",
      "type": "string"
    },
    "debug_markers": {
      "default": [],
      "description": "Identifiers to generate verbose debug statements for",
      "type": "list(string)"
    },
    "defaults": {
      "default": {},
      "type": "any"
    },
    "local_module_source": {
      "default": null,
      "type": "any"
    },
    "log_file_name": {
      "default": "run.log",
      "description": "Port library parameter",
      "type": "string"
    },
    "map_name_to": {
      "default": {},
      "type": "any"
    },
    "map_sanitized_name_to": {
      "default": {},
      "type": "any"
    },
    "overrides": {
      "default": {},
      "type": "any"
    },
    "parameter_generators": {
      "default": {},
      "type": "any"
    },
    "params": {
      "default": {},
      "description": "Override for multiple params",
      "type": "any"
    },
    "repository_name": {
      "type": "any"
    },
    "repository_tag": {
      "type": "any"
    },
    "requires_github_authentication": {
      "default": false,
      "type": "bool"
    },
    "variable_files": {
      "type": "any"
    },
    "verbose": {
      "default": false,
      "description": "Port library parameter",
      "type": "bool"
    },
    "verbosity": {
      "default": 1,
      "description": "Port library parameter",
      "type": "number"
    }
  }
}