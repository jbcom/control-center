{
  "data": {
    "env_var": {
      "EXECUTION_ROLE_ARN": {
        "id": "EXECUTION_ROLE_ARN",
        "required": false
      },
      "ROLE_SESSION_NAME": {
        "id": "ROLE_SESSION_NAME",
        "required": false
      }
    },
    "external": {
      "default": {
        "program": [
          "tm_cli",
          "get_aws_sso_account_assignments"
        ],
        "query": {
          "EXECUTION_ROLE_ARN": "${data.env_var.EXECUTION_ROLE_ARN.value}",
          "ROLE_SESSION_NAME": "${data.env_var.ROLE_SESSION_NAME.value}",
          "account_id": "${try(nonsensitive(var.account_id), var.account_id)}",
          "checksum": "${try(nonsensitive(var.checksum), var.checksum)}",
          "debug_markers": "${try(nonsensitive(jsonencode(var.debug_markers)), jsonencode(var.debug_markers))}",
          "groups": "${try(nonsensitive(base64encode(jsonencode(var.groups))), base64encode(jsonencode(var.groups)))}",
          "log_file_name": "${try(nonsensitive(var.log_file_name), var.log_file_name)}",
          "params": "${try(nonsensitive(base64encode(jsonencode(var.params))), base64encode(jsonencode(var.params)))}",
          "permission_set_arn": "${try(nonsensitive(var.permission_set_arn), var.permission_set_arn)}",
          "sort_by_name": "${try(nonsensitive(var.sort_by_name), var.sort_by_name)}",
          "users": "${try(nonsensitive(base64encode(jsonencode(var.users))), base64encode(jsonencode(var.users)))}",
          "verbose": "${try(nonsensitive(var.verbose), var.verbose)}",
          "verbosity": "${try(nonsensitive(var.verbosity), var.verbosity)}"
        }
      }
    }
  },
  "locals": {
    "results": "${jsondecode(base64decode(data.external.default.result[\"assignments\"]))}"
  },
  "output": {
    "assignments": {
      "description": "Data query results",
      "value": "${local.results}"
    }
  },
  "terraform": {
    "required_providers": {
      "env": {
        "source": "tcarreira/env",
        "version": ">=0.2.0"
      },
      "external": {
        "source": "hashicorp/external",
        "version": ">=2.3.1"
      }
    },
    "required_version": ">=1.6"
  },
  "variable": {
    "account_id": {
      "type": "string"
    },
    "checksum": {
      "default": "",
      "description": "Optional checksum to use for triggering resource updates",
      "type": "string"
    },
    "debug_markers": {
      "default": [],
      "description": "Identifiers to generate verbose debug statements for",
      "type": "list(string)"
    },
    "groups": {
      "default": {},
      "type": "any"
    },
    "log_file_name": {
      "default": "run.log",
      "description": "Port library parameter",
      "type": "string"
    },
    "params": {
      "default": {},
      "description": "Override for multiple params",
      "type": "any"
    },
    "permission_set_arn": {
      "type": "string"
    },
    "sort_by_name": {
      "default": false,
      "type": "bool"
    },
    "users": {
      "default": {},
      "type": "any"
    },
    "verbose": {
      "default": false,
      "description": "Port library parameter",
      "type": "bool"
    },
    "verbosity": {
      "default": 1,
      "description": "Port library parameter",
      "type": "number"
    }
  }
}