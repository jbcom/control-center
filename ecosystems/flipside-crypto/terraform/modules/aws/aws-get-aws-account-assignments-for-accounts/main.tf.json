{
  "module": {
    "default": {
      "account_id": "${each.key}",
      "checksum": "${try(nonsensitive(var.checksum), var.checksum)}",
      "debug_markers": "${try(nonsensitive(var.debug_markers), var.debug_markers)}",
      "for_each": "${try(nonsensitive(var.aws_accounts), var.aws_accounts)}",
      "groups": "${try(nonsensitive(var.groups), var.groups)}",
      "log_file_name": "${try(nonsensitive(var.log_file_name), var.log_file_name)}",
      "params": "${try(nonsensitive(var.params), var.params)}",
      "permission_set_arn": "${try(nonsensitive(var.permission_set_arn), var.permission_set_arn)}",
      "sort_by_name": "${try(nonsensitive(var.sort_by_name), var.sort_by_name)}",
      "source": "../aws-get-aws-sso-account-assignments",
      "users": "${try(nonsensitive(var.users), var.users)}",
      "verbose": "${try(nonsensitive(var.verbose), var.verbose)}",
      "verbosity": "${try(nonsensitive(var.verbosity), var.verbosity)}"
    }
  },
  "output": {
    "assignments": {
      "description": "Data query results",
      "value": "${{for id, data in module.default : id => data.assignments}}"
    }
  },
  "variable": {
    "aws_accounts": {
      "type": "any"
    },
    "checksum": {
      "default": "",
      "description": "Optional checksum to use for triggering resource updates",
      "type": "string"
    },
    "debug_markers": {
      "default": [],
      "description": "Identifiers to generate verbose debug statements for",
      "type": "list(string)"
    },
    "groups": {
      "default": {},
      "type": "any"
    },
    "log_file_name": {
      "default": "run.log",
      "description": "Port library parameter",
      "type": "string"
    },
    "params": {
      "default": {},
      "description": "Override for multiple params",
      "type": "any"
    },
    "permission_set_arn": {
      "type": "string"
    },
    "sort_by_name": {
      "default": false,
      "type": "bool"
    },
    "users": {
      "default": {},
      "type": "any"
    },
    "verbose": {
      "default": false,
      "description": "Port library parameter",
      "type": "bool"
    },
    "verbosity": {
      "default": 1,
      "description": "Port library parameter",
      "type": "number"
    }
  }
}