{
  "data": {
    "env_sensitive": {
      "ACTIONS_ID_TOKEN_REQUEST_TOKEN": {
        "id": "ACTIONS_ID_TOKEN_REQUEST_TOKEN",
        "required": false
      },
      "VAULT_ROLE_ID": {
        "id": "VAULT_ROLE_ID",
        "required": false
      },
      "VAULT_SECRET_ID": {
        "id": "VAULT_SECRET_ID",
        "required": false
      },
      "VAULT_TOKEN": {
        "id": "VAULT_TOKEN",
        "required": false
      }
    },
    "env_var": {
      "ACTIONS_ID_TOKEN_REQUEST_URL": {
        "id": "ACTIONS_ID_TOKEN_REQUEST_URL",
        "required": false
      },
      "GITHUB_ACTIONS": {
        "id": "GITHUB_ACTIONS",
        "required": false
      },
      "VAULT_APPROLE_PATH": {
        "id": "VAULT_APPROLE_PATH",
        "required": false
      },
      "VAULT_NAMESPACE": {
        "id": "VAULT_NAMESPACE",
        "required": false
      },
      "VAULT_URL": {
        "id": "VAULT_URL",
        "required": false
      }
    },
    "external": {
      "default": {
        "program": [
          "tm_cli",
          "list_vault_secrets"
        ],
        "query": {
          "ACTIONS_ID_TOKEN_REQUEST_TOKEN": "${data.env_sensitive.ACTIONS_ID_TOKEN_REQUEST_TOKEN.value}",
          "ACTIONS_ID_TOKEN_REQUEST_URL": "${data.env_var.ACTIONS_ID_TOKEN_REQUEST_URL.value}",
          "GITHUB_ACTIONS": "${data.env_var.GITHUB_ACTIONS.value}",
          "VAULT_APPROLE_PATH": "${data.env_var.VAULT_APPROLE_PATH.value}",
          "VAULT_NAMESPACE": "${data.env_var.VAULT_NAMESPACE.value}",
          "VAULT_ROLE_ID": "${data.env_sensitive.VAULT_ROLE_ID.value}",
          "VAULT_SECRET_ID": "${data.env_sensitive.VAULT_SECRET_ID.value}",
          "VAULT_TOKEN": "${data.env_sensitive.VAULT_TOKEN.value}",
          "VAULT_URL": "${data.env_var.VAULT_URL.value}",
          "checksum": "${try(nonsensitive(var.checksum), var.checksum)}",
          "debug_markers": "${try(nonsensitive(jsonencode(var.debug_markers)), jsonencode(var.debug_markers))}",
          "log_file_name": "${try(nonsensitive(var.log_file_name), var.log_file_name)}",
          "max_depth": "${try(nonsensitive(var.max_depth), var.max_depth)}",
          "mount_point": "${try(nonsensitive(var.mount_point), var.mount_point)}",
          "params": "${try(nonsensitive(base64encode(jsonencode(var.params))), base64encode(jsonencode(var.params)))}",
          "root_path": "${try(nonsensitive(var.root_path), var.root_path)}",
          "verbose": "${try(nonsensitive(var.verbose), var.verbose)}",
          "verbosity": "${try(nonsensitive(var.verbosity), var.verbosity)}"
        }
      }
    }
  },
  "locals": {
    "results": "${jsondecode(base64decode(data.external.default.result[\"secrets\"]))}"
  },
  "output": {
    "secrets": {
      "description": "Data query results",
      "value": "${local.results}"
    }
  },
  "terraform": {
    "required_providers": {
      "env": {
        "source": "tcarreira/env",
        "version": ">=0.2.0"
      },
      "external": {
        "source": "hashicorp/external",
        "version": ">=2.3.1"
      }
    },
    "required_version": ">=1.6"
  },
  "variable": {
    "checksum": {
      "default": "",
      "description": "Optional checksum to use for triggering resource updates",
      "type": "string"
    },
    "debug_markers": {
      "default": [],
      "description": "Identifiers to generate verbose debug statements for",
      "type": "list(string)"
    },
    "log_file_name": {
      "default": "run.log",
      "description": "Port library parameter",
      "type": "string"
    },
    "max_depth": {
      "default": null,
      "type": "number"
    },
    "mount_point": {
      "default": null,
      "type": "string"
    },
    "params": {
      "default": {},
      "description": "Override for multiple params",
      "type": "any"
    },
    "root_path": {
      "default": "/",
      "type": "string"
    },
    "verbose": {
      "default": false,
      "description": "Port library parameter",
      "type": "bool"
    },
    "verbosity": {
      "default": 1,
      "description": "Port library parameter",
      "type": "number"
    }
  }
}