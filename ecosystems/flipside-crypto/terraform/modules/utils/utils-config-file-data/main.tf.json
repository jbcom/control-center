{
  "data": {
    "external": {
      "default": {
        "program": [
          "tm_cli",
          "config_file_data"
        ],
        "query": {
          "allowed_extensions": "${try(nonsensitive(jsonencode(var.allowed_extensions)), jsonencode(var.allowed_extensions))}",
          "checksum": "${try(nonsensitive(var.checksum), var.checksum)}",
          "config": "${try(nonsensitive(base64encode(jsonencode(var.config))), base64encode(jsonencode(var.config)))}",
          "config_dir": "${try(nonsensitive(var.config_dir), var.config_dir)}",
          "config_dirs": "${try(nonsensitive(jsonencode(var.config_dirs)), jsonencode(var.config_dirs))}",
          "config_files_match": "${try(nonsensitive(var.config_files_match), var.config_files_match)}",
          "config_glob": "${try(nonsensitive(var.config_glob), var.config_glob)}",
          "debug_markers": "${try(nonsensitive(jsonencode(var.debug_markers)), jsonencode(var.debug_markers))}",
          "denied_extensions": "${try(nonsensitive(jsonencode(var.denied_extensions)), jsonencode(var.denied_extensions))}",
          "log_file_name": "${try(nonsensitive(var.log_file_name), var.log_file_name)}",
          "nest_config_under_key": "${try(nonsensitive(var.nest_config_under_key), var.nest_config_under_key)}",
          "ordered_config_merge": "${try(nonsensitive(var.ordered_config_merge), var.ordered_config_merge)}",
          "params": "${try(nonsensitive(base64encode(jsonencode(var.params))), base64encode(jsonencode(var.params)))}",
          "verbose": "${try(nonsensitive(var.verbose), var.verbose)}",
          "verbosity": "${try(nonsensitive(var.verbosity), var.verbosity)}"
        }
      }
    }
  },
  "locals": {
    "results": "${jsondecode(base64decode(data.external.default.result[\"config\"]))}"
  },
  "output": {
    "config": {
      "description": "Data query results",
      "value": "${local.results}"
    }
  },
  "terraform": {
    "required_providers": {
      "env": {
        "source": "tcarreira/env",
        "version": ">=0.2.0"
      },
      "external": {
        "source": "hashicorp/external",
        "version": ">=2.3.1"
      }
    },
    "required_version": ">=1.6"
  },
  "variable": {
    "allowed_extensions": {
      "default": [],
      "type": "any"
    },
    "checksum": {
      "default": "",
      "description": "Optional checksum to use for triggering resource updates",
      "type": "string"
    },
    "config": {
      "default": {},
      "type": "any"
    },
    "config_dir": {
      "default": null,
      "type": "string"
    },
    "config_dirs": {
      "default": [],
      "type": "any"
    },
    "config_files_match": {
      "default": null,
      "type": "string"
    },
    "config_glob": {
      "default": null,
      "type": "string"
    },
    "debug_markers": {
      "default": [],
      "description": "Identifiers to generate verbose debug statements for",
      "type": "list(string)"
    },
    "denied_extensions": {
      "default": [],
      "type": "any"
    },
    "log_file_name": {
      "default": "run.log",
      "description": "Port library parameter",
      "type": "string"
    },
    "nest_config_under_key": {
      "default": null,
      "type": "string"
    },
    "ordered_config_merge": {
      "default": true,
      "type": "bool"
    },
    "params": {
      "default": {},
      "description": "Override for multiple params",
      "type": "any"
    },
    "verbose": {
      "default": false,
      "description": "Port library parameter",
      "type": "bool"
    },
    "verbosity": {
      "default": 1,
      "description": "Port library parameter",
      "type": "number"
    }
  }
}