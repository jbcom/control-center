{
  "data": {
    "external": {
      "default": {
        "program": [
          "tm_cli",
          "flatmap"
        ],
        "query": {
          "checksum": "${try(nonsensitive(var.checksum), var.checksum)}",
          "debug_markers": "${try(nonsensitive(jsonencode(var.debug_markers)), jsonencode(var.debug_markers))}",
          "delimiter": "${try(nonsensitive(var.delimiter), var.delimiter)}",
          "log_file_name": "${try(nonsensitive(var.log_file_name), var.log_file_name)}",
          "params": "${try(nonsensitive(base64encode(jsonencode(var.params))), base64encode(jsonencode(var.params)))}",
          "source_map": "${try(nonsensitive(base64encode(jsonencode(var.source_map))), base64encode(jsonencode(var.source_map)))}",
          "use_all_ancestors_in_child_keys": "${try(nonsensitive(var.use_all_ancestors_in_child_keys), var.use_all_ancestors_in_child_keys)}",
          "use_parent_in_child_keys": "${try(nonsensitive(var.use_parent_in_child_keys), var.use_parent_in_child_keys)}",
          "verbose": "${try(nonsensitive(var.verbose), var.verbose)}",
          "verbosity": "${try(nonsensitive(var.verbosity), var.verbosity)}"
        }
      }
    }
  },
  "locals": {
    "results": "${jsondecode(base64decode(data.external.default.result[\"flattened_map\"]))}"
  },
  "output": {
    "flattened_map": {
      "description": "Data query results",
      "value": "${local.results}"
    }
  },
  "terraform": {
    "required_providers": {
      "env": {
        "source": "tcarreira/env",
        "version": ">=0.2.0"
      },
      "external": {
        "source": "hashicorp/external",
        "version": ">=2.3.1"
      }
    },
    "required_version": ">=1.6"
  },
  "variable": {
    "checksum": {
      "default": "",
      "description": "Optional checksum to use for triggering resource updates",
      "type": "string"
    },
    "debug_markers": {
      "default": [],
      "description": "Identifiers to generate verbose debug statements for",
      "type": "list(string)"
    },
    "delimiter": {
      "default": "_",
      "type": "string"
    },
    "log_file_name": {
      "default": "run.log",
      "description": "Port library parameter",
      "type": "string"
    },
    "params": {
      "default": {},
      "description": "Override for multiple params",
      "type": "any"
    },
    "source_map": {
      "type": "any"
    },
    "use_all_ancestors_in_child_keys": {
      "default": false,
      "type": "bool"
    },
    "use_parent_in_child_keys": {
      "default": false,
      "type": "bool"
    },
    "verbose": {
      "default": false,
      "description": "Port library parameter",
      "type": "bool"
    },
    "verbosity": {
      "default": 1,
      "description": "Port library parameter",
      "type": "number"
    }
  }
}