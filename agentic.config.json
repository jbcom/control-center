{
  "$schema": "https://agentic-control.dev/schema/config.json",
  "tokens": {
    "organizations": {
      "jbcom": {
        "name": "jbcom",
        "tokenEnvVar": "GITHUB_TOKEN",
        "defaultBranch": "main",
        "isEnterprise": false
      },
      "FlipsideCrypto": {
        "name": "FlipsideCrypto",
        "tokenEnvVar": "GITHUB_TOKEN",
        "defaultBranch": "main",
        "isEnterprise": true
      },
      "fsc-platform": {
        "name": "fsc-platform",
        "tokenEnvVar": "GITHUB_TOKEN",
        "defaultBranch": "main",
        "isEnterprise": true
      },
      "fsc-internal-tooling-administration": {
        "name": "fsc-internal-tooling-administration",
        "tokenEnvVar": "GITHUB_TOKEN",
        "defaultBranch": "main",
        "isEnterprise": true
      }
    },
    "defaultTokenEnvVar": "GITHUB_TOKEN",
    "prReviewTokenEnvVar": "GITHUB_TOKEN"
  },
  "defaultModel": "claude-sonnet-4-5-20250929",
  "defaultRepository": "jbcom/jbcom-control-center",
  "logLevel": "info",
  "agents": {
    "terraform-repository-manager": {
      "name": "Terraform Repository Manager",
      "description": "Manages GitHub repository configurations using Terraform Cloud and MCP server",
      "instructionsFile": ".github/agents/terraform-repository-manager.md",
      "requiredTools": [
        "terraform-list_workspaces",
        "terraform-get_workspace_details", 
        "terraform-create_run",
        "terraform-list_runs",
        "terraform-get_run_details",
        "terraform-list_workspace_variables",
        "terraform-create_workspace_variable",
        "terraform-search_providers",
        "terraform-get_latest_provider_version"
      ],
      "requiredSecrets": [
        "TF_API_TOKEN",
        "CI_GITHUB_TOKEN"
      ],
      "workspace": "jbcom/jbcom-control-center",
      "autoTriggers": [
        "drift-detection",
        "repository-config-change"
      ]
    }
  }
}
