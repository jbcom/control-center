{
  "$schema": "https://agentic-control.dev/schema/config.json",
  "tokens": {
    "organizations": {
      "jbcom": {
        "name": "jbcom",
        "tokenEnvVar": "GITHUB_TOKEN",
        "defaultBranch": "main",
        "isEnterprise": false
      }
    },
    "defaultTokenEnvVar": "GITHUB_TOKEN",
    "prReviewTokenEnvVar": "GITHUB_TOKEN"
  },
  "defaultModel": "claude-sonnet-4-5-20250929",
  "defaultRepository": "jbcom/control-center",
  "logLevel": "info",
  "crewDirectory": ".crew",
  "agents": {
    "triage": {
      "description": "Issue and PR triage across the ecosystem",
      "configPath": ".crew/config/agents.yaml",
      "tasksPath": ".crew/config/tasks.yaml",
      "defaultTask": "assess_new_issue",
      "tools": [
        "github_issues",
        "github_prs",
        "mcp_filesystem"
      ]
    },
    "ecosystem": {
      "description": "Cross-repo dependency and sync coordination",
      "configPath": ".crew/config/agents.yaml",
      "tasksPath": ".crew/config/tasks.yaml",
      "defaultTask": "check_dependency_graph",
      "tools": [
        "github_repos",
        "github_releases",
        "github_actions",
        "mcp_filesystem"
      ]
    },
    "planning": {
      "description": "Sprint and roadmap management",
      "configPath": ".crew/config/agents.yaml",
      "tasksPath": ".crew/config/tasks.yaml",
      "defaultTask": "generate_weekly_sprint",
      "tools": [
        "github_issues",
        "github_projects",
        "github_milestones"
      ]
    }
  },
  "ecosystem": {
    "killList": {
      "description": "Patterns for files/directories to delete across all managed repos during sync",
      "patterns": [
        {
          "pattern": ".github/workflows/ecosystem-*.yml",
          "type": "glob",
          "reason": "Consolidated into triage.yml, autoheal.yml, review.yml, delegator.yml",
          "exceptions": []
        },
        {
          "pattern": ".github/workflows/ai-*.yml",
          "type": "glob",
          "reason": "Consolidated into core workflows using control-center binary",
          "exceptions": []
        },
        {
          "pattern": ".github/workflows/*-local.yml",
          "type": "glob",
          "reason": "No more local variants - all workflows use control-center binary",
          "exceptions": []
        },
        {
          "pattern": ".github/workflows/jules-*.yml",
          "type": "glob",
          "reason": "Jules remote agent removed - agents work within PRs using control-center",
          "exceptions": []
        },
        {
          "pattern": "^.github/workflows/(ecosystem|ai)-(curator|delegator|fixer|reviewer|sage|triage|harvester|surveyor|assessment|control|merge|orchestrator)",
          "type": "regex",
          "reason": "All AI/ecosystem workflows consolidated into 4 core workflows",
          "exceptions": [
            ".github/workflows/ecosystem-sync.yml"
          ]
        },
        {
          "pattern": ".crew/agents/jules/**",
          "type": "glob",
          "reason": "Jules configuration removed",
          "exceptions": []
        }
      ],
      "protectedPatterns": [
        ".github/workflows/triage.yml",
        ".github/workflows/autoheal.yml",
        ".github/workflows/review.yml",
        ".github/workflows/delegator.yml",
        ".github/workflows/ci.yml",
        ".github/workflows/go.yml",
        ".github/workflows/docs.yml",
        ".github/workflows/docs-sync.yml",
        ".github/workflows/release.yml",
        ".github/workflows/release-please.yml",
        ".github/workflows/test-coverage.yml",
        ".github/workflows/control-center-build.yml",
        ".github/workflows/ecosystem-sync.yml"
      ]
    },
    "managedRepos": [
      "jbcom/nodejs-agentic-control",
      "jbcom/nodejs-agentic-triage",
      "jbcom/python-agentic-crew",
      "jbcom/python-vendor-connectors",
      "jbcom/python-extended-data-types",
      "jbcom/python-directed-inputs-class",
      "jbcom/python-lifecyclelogging",
      "jbcom/python-terraform-bridge",
      "jbcom/python-rivers-of-reckoning",
      "jbcom/python-ai-game-dev",
      "jbcom/nodejs-strata",
      "jbcom/nodejs-pixels-pygame-palace",
      "jbcom/nodejs-otter-river-rush",
      "jbcom/nodejs-otterfall",
      "jbcom/nodejs-rivermarsh",
      "jbcom/nodejs-strata-capacitor-plugin",
      "jbcom/nodejs-strata-examples",
      "jbcom/nodejs-strata-presets",
      "jbcom/nodejs-strata-react-native-plugin",
      "jbcom/nodejs-strata-shaders",
      "jbcom/nodejs-strata-typescript-tutor",
      "jbcom/go-port-api",
      "jbcom/go-secretsync",
      "jbcom/go-vault-secret-sync",
      "jbcom/terraform-github-markdown",
      "jbcom/terraform-repository-automation"
    ],
    "dependencyGraph": {
      "nodejs-agentic-control": {
        "consumes": [],
        "consumers": [
          "nodejs-agentic-triage"
        ]
      },
      "nodejs-agentic-triage": {
        "consumes": [
          "nodejs-agentic-control"
        ],
        "consumers": []
      },
      "python-agentic-crew": {
        "consumes": [
          "python-vendor-connectors",
          "python-extended-data-types",
          "python-directed-inputs-class"
        ],
        "consumers": []
      },
      "python-vendor-connectors": {
        "consumes": [
          "python-lifecyclelogging"
        ],
        "consumers": [
          "python-agentic-crew"
        ]
      },
      "python-extended-data-types": {
        "consumes": [],
        "consumers": [
          "python-vendor-connectors",
          "python-agentic-crew",
          "python-directed-inputs-class"
        ]
      },
      "python-directed-inputs-class": {
        "consumes": [
          "python-extended-data-types"
        ],
        "consumers": [
          "python-agentic-crew"
        ]
      },
      "python-lifecyclelogging": {
        "consumes": [],
        "consumers": [
          "python-vendor-connectors",
          "python-agentic-crew",
          "python-terraform-bridge"
        ]
      },
      "nodejs-strata": {
        "consumes": [],
        "consumers": [
          "nodejs-rivermarsh",
          "nodejs-otter-river-rush"
        ]
      }
    },
    "projects": {
      "integration": "PVT_kwHOAChyN84BJLOn",
      "roadmap": "PVT_kwHOAChyN84BKeDX"
    }
  }
}