{
  "name": "cursor-background-agent-mcp-server",
  "version": "1.0.3",
  "description": "MCP Server for Cursor Background Agents API - run autonomous coding agents from any MCP client",
  "main": "dist/mcpStdioServer.js",
  "bin": {
    "cursor-background-agent-mcp-server": "dist/mcpStdioServer.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "mcp": "node dist/mcpStdioServer.js",
    "dev": "ts-node src/server.ts",
    "dev:mcp": "ts-node src/mcpStdioServer.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "keywords": [
    "mcp",
    "cursor",
    "background-agents",
    "api",
    "typescript",
    "model-context-protocol",
    "vscode",
    "ai",
    "automation"
  ],
  "author": "MCP Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/samuelbalogh/cursor-background-agent-mcp.git"
  },
  "homepage": "https://github.com/samuelbalogh/cursor-background-agent-mcp#readme",
  "bugs": {
    "url": "https://github.com/samuelbalogh/cursor-background-agent-mcp/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "cursor-background-agent-mcp-server": "^1.0.0",
    "express": "^4.21.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
